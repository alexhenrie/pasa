{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "2.0.1",
  "description": "__MSG_description__",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "64": "icon-64.png"
    },
    "default_title": "__MSG_actionTitle__"
  },
  "web_accessible_resources": [
    "bootstrap.min.css",
    "generate.html",
    "generate.js"
  ],
  "default_locale": "en",
  "permissions": [
    "clipboardWrite",
    "privacy",
    "storage"
  ]
}
